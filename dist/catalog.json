{
  "kind": "List",
  "apiVersion": "v1",
  "items": [
    {
      "apiVersion": "camel.apache.org/v1alpha1",
      "kind": "Kamelet",
      "metadata": {
        "name": "chuck-source",
        "annotations": {
          "camel.apache.org/kamelet.icon": "data:image/svg+xml;base64,"
        },
        "labels": {
          "camel.apache.org/kamelet.type": "source"
        }
      },
      "spec": {
        "definition": {
          "title": "Chuck Source",
          "description": "Produces random sentences about Chuck Norris",
          "properties": {
            "period": {
              "title": "Period",
              "description": "The interval between two events",
              "type": "integer",
              "default": 10000
            }
          }
        },
        "types": {
          "out": {
            "mediaType": "application/json"
          }
        },
        "flow": {
          "from": {
            "uri": "timer:tick",
            "parameters": {
              "period": "#property:period"
            },
            "steps": [
              {
                "to": "https://api.icndb.com/jokes/random"
              },
              {
                "to": "direct:#property:routeId"
              }
            ]
          }
        }
      }
    },
    {
      "apiVersion": "camel.apache.org/v1alpha1",
      "kind": "Kamelet",
      "metadata": {
        "name": "http-sink",
        "annotations": {
          "camel.apache.org/kamelet.icon": "data:image/svg+xml;base64,"
        },
        "labels": {
          "camel.apache.org/kamelet.type": "sink"
        }
      },
      "spec": {
        "definition": {
          "title": "HTTP Sink",
          "description": "Forward events to an HTTP endpoint",
          "required": [
            "url"
          ],
          "properties": {
            "url": {
              "title": "URL",
              "description": "The destination HTTP/HTTPS URL",
              "type": "string",
              "example": "http://www.company.com/api/endpoint"
            }
          }
        },
        "types": {
          "in": {},
          "out": {}
        },
        "flow": {
          "from": {
            "uri": "direct:#property:routeId",
            "steps": [
              {
                "to": "netty-http:#property:url"
              }
            ]
          }
        }
      }
    },
    {
      "apiVersion": "camel.apache.org/v1alpha1",
      "kind": "Kamelet",
      "metadata": {
        "name": "telegram-source",
        "annotations": {
          "camel.apache.org/kamelet.icon": "data:image/svg+xml;base64,"
        },
        "labels": {
          "camel.apache.org/kamelet.type": "source"
        }
      },
      "spec": {
        "definition": {
          "title": "Telegram Source",
          "description": "Receive all messages that people send to your telegram bot.",
          "required": [
            "botToken"
          ],
          "properties": {
            "botToken": {
              "title": "Token",
              "description": "The token to access your bot on Telegram",
              "type": "string",
              "x-descriptors": [
                "urn:alm:descriptor:com.tectonic.ui:password"
              ],
              "example": "token-from-bot-father"
            }
          }
        },
        "types": {
          "out": {
            "mediaType": "application/json"
          }
        },
        "dependencies": [
          "camel:jackson"
        ],
        "flow": {
          "from": {
            "uri": "telegram:bots",
            "parameters": {
              "authorizationToken": "#property:botToken"
            },
            "steps": [
              {
                "marshal": {
                  "json": {}
                }
              },
              {
                "to": "direct:#property:routeId"
              }
            ]
          }
        }
      }
    },
    {
      "apiVersion": "camel.apache.org/v1alpha1",
      "kind": "Kamelet",
      "metadata": {
        "name": "timer-source",
        "annotations": {
          "camel.apache.org/kamelet.icon": "data:image/svg+xml;base64,"
        },
        "labels": {
          "camel.apache.org/kamelet.type": "source"
        }
      },
      "spec": {
        "definition": {
          "title": "Timer Source",
          "description": "Produces periodic events with a custom payload",
          "required": [
            "message"
          ],
          "properties": {
            "period": {
              "title": "Period",
              "description": "The interval between two events",
              "type": "integer",
              "default": 1000
            },
            "message": {
              "title": "Message",
              "description": "The message to generate",
              "type": "string",
              "example": "hello world"
            }
          }
        },
        "types": {
          "out": {
            "mediaType": "text/plain"
          }
        },
        "flow": {
          "from": {
            "uri": "timer:tick",
            "parameters": {
              "period": "#property:period"
            },
            "steps": [
              {
                "set-body": {
                  "constant": "#property:message"
                }
              },
              {
                "to": "direct:#property:routeId"
              }
            ]
          }
        }
      }
    }
  ]
}
